Texture2D particleTexture;
SamplerState sampleType;

struct PixelInput
{
	float4 position : SV_POSITION;
	float2 tex : TEXCOORD0;
};

float4 ParticlePixelShader(PixelInput input) : SV_TARGET
{
	float4 texCol;

	texCol = particleTexture.Sample(sampleType, input.tex);
	if(texCol.a < .3)
		discard;

	return texCol;
}